{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "./patches/", "cspell.json"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": ["codeowner", "commonality", "ollama", "pawelblaszczyk", "pgvector", "stylesheet", "turbopack", "typecheck"],
	"enableGlobDot": true,
	"enableFiletypes": ["dockercompose", "dockerfile", "github-actions-workflow"],
	"ignoreWords": [
		"commonalityco",
		"dropcursor",
		"gapcursor",
		"isbot",
		"jsxdev",
		"langchain",
		"nuqs",
		"pipeable",
		"replicache",
		"taze",
		"textsplitters",
		"tiptap",
		"tseslint",
		"xyflow"
	]
}
